  "[go][c][cpp][json][rust][markdown][typescript][typescriptreact][javascript][python][html]": {
    "coc.preferences.formatOnSave": true
  },
  "suggest.noselect": true,
  "suggest.triggerAfterInsertEnter": true,

  "python.linting.enabled": true,
